@model IEnumerable<ArtWebApp.DataModels.MarkerMadeMaster>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/ArtMVC/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MarkerMade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MarkerMade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CuttingMarkerMadeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CuttingMarkerMadeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CuttingMarkerMadeID })
        </td>
    </tr>
}

</table>
